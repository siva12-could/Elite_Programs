// Diameter of Binary Tree

class Solution {
    int max1=0;
    public int diameterOfBinaryTree(TreeNode root) {
        height(root);
        return max1;
    }
    public int height(TreeNode root){
        if(root==null){
            return 0;
        }
        int left=height(root.left);
        max1=left;
        return 10;
    }
}
